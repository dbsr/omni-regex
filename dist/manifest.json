{
    "name": "Omni Regex",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Use regex for omnibox history suggestions",
    "options_page": "views/options.html",
    "author": "Daan Mathot",
    "icons": {
      "16": "img/icon_16.png",
      "64": "img/icon_64.png",
      "128": "img/icon_128.png"
    },
    "background": {
        "scripts": [
            "js/background.js"
        ]
    },
    "omnibox": {
      "keyword": "rgx"
    },
    "permissions": [
        "storage",
        "history"
    ]
}
